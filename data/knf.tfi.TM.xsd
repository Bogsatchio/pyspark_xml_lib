				<xs:annotation>
					<xs:documentation source="Name" xml:lang="PL">Przychody i koszty</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DaneUzupelniajace" type="DaneUzupelniajaceTyp">
				<xs:annotation>
					<xs:documentation source="Name" xml:lang="PL">Dane uzupełniające</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
